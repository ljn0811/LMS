package kr.happyjob.study.adm.model;

public class EquipmentVO {
	private int tool_no;
	private int classroom_no;
	private String tool_nm;
	private int tool_cnt;
	private String tool_etc;
	private String classroom_nm;
	
	
	
	public String getClassroom_nm() {
		return classroom_nm;
	}
	public void setClassroom_nm(String classroom_nm) {
		this.classroom_nm = classroom_nm;
	}
	public int getTool_no() {
		return tool_no;
	}
	public void setTool_no(int tool_no) {
		this.tool_no = tool_no;
	}
	public int getClassroom_no() {
		return classroom_no;
	}
	public void setClassroom_no(int classroom_no) {
		this.classroom_no = classroom_no;
	}
	public String getTool_nm() {
		return tool_nm;
	}
	public void setTool_nm(String tool_nm) {
		this.tool_nm = tool_nm;
	}
	public int getTool_cnt() {
		return tool_cnt;
	}
	public void setTool_cnt(int tool_cnt) {
		this.tool_cnt = tool_cnt;
	}
	public String getTool_etc() {
		return tool_etc;
	}
	public void setTool_etc(String tool_etc) {
		this.tool_etc = tool_etc;
	}

	
	
}
