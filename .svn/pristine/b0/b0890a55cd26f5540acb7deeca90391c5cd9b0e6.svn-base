package kr.happyjob.study.std.model;


public class LearningMaterialsStdVO{	
	
	//학습자료번호
    private int material_no;
    //회원번호
    private int user_no;
    //제목
    private String material_title;
    //내용
    private String material_content;
    //등록일
    private String created_at;
    //조회수
    private int material_views;
    //파일번호
    private int file_no;
    //파일이름
    private String file_name;
    //파일크기
    private int file_size;
    //파일확장자
    private String file_ext;
    //파일경로
    private String file_path;
    //논리경로
    private String relative_path;
    //유저이름
    private String name;
    
    // getter and setter
    public int getMaterial_no() {
		return material_no;
	}
	public void setMaterial_no(int material_no) {
		this.material_no = material_no;
	}
	public int getUser_no() {
		return user_no;
	}
	public void setUser_no(int user_no) {
		this.user_no = user_no;
	}
	public String getMaterial_title() {
		return material_title;
	}
	public void setMaterial_title(String material_title) {
		this.material_title = material_title;
	}
	public String getMaterial_content() {
		return material_content;
	}
	public void setMaterial_content(String material_content) {
		this.material_content = material_content;
	}
	public String getCreated_at() {
		return created_at;
	}
	public void setCreated_at(String created_at) {
		this.created_at = created_at;
	}
	public int getMaterial_views() {
		return material_views;
	}
	public void setMaterial_views(int material_views) {
		this.material_views = material_views;
	}
	public int getFile_no() {
		return file_no;
	}
	public void setFile_no(int file_no) {
		this.file_no = file_no;
	}
	public String getFile_name() {
		return file_name;
	}
	public void setFile_name(String file_name) {
		this.file_name = file_name;
	}
	public int getFile_size() {
		return file_size;
	}
	public void setFile_size(int file_size) {
		this.file_size = file_size;
	}
	public String getFile_ext() {
		return file_ext;
	}
	public void setFile_ext(String file_ext) {
		this.file_ext = file_ext;
	}
	public String getFile_path() {
		return file_path;
	}
	public void setFile_path(String file_path) {
		this.file_path = file_path;
	}
	public String getRelative_path() {
		return relative_path;
	}
	public void setRelative_path(String relative_path) {
		this.relative_path = relative_path;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
