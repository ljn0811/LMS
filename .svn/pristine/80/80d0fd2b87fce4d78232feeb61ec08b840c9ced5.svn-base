		return lecContent;
	}
	public void setLecContent(String lecContent) {
		this.lecContent = lecContent;
	}
	public int getLearnGoal() {
		return learnGoal;
	}
	public void setLearnGoal(int learnGoal) {
		this.learnGoal = learnGoal;
	}
	public String getLearnPlan() {
		return learnPlan;
	}
	public void setLearnPlan(String learnPlan) {
		this.learnPlan = learnPlan;
	}

